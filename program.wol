import std;
#if DOTNET
	use System;
	funcalias print = Console.WriteLine;
#if JVM
	use java.lang;
	typealias string = String;
	typealias bool = Boolean;
	funcalias print = System.out.println;
#endif 
 
fun print(string text) 
{
	#if DOTNET
		Console.WriteLine(text);
	#if JVM
		System.out.println(text);
	#endif
}

print("Hello World");